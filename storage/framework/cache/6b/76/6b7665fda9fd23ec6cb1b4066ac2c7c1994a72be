    $('#showCalendly').click(function(e){
        e.preventDefault();
        if($('#calendlyModal').hasClass('active')){
            $('#calendlyModal').removeClass('active');
        } else {
            $('#calendlyModal').addClass('active');    
        }
    });
    $('#closeCalendly').click(function(e){
        e.preventDefault();
        $('#calendlyModal').removeClass('active');
    });
